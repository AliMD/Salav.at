{
  "entrypoint": "index.html",
  "shell": "js/salavat-pwa.js",
  "sources": [
    "image/**/*",
    "font/font.css",
    "font/iran-sans/iran-sans-regular.woff",
    "font/iran-sans/iran-sans-bold.woff",
    "font/roboto/Roboto-Regular.woff"
  ],
  "extraDependencies": [
    "manifest.json",
    "push-manifest.json",
    "font/**/*",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "npm": true,
  "moduleResolution": "node",
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": "es2015",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es5-bundled",
      "js": {
        "compile": "es5",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    }
  ]
}
